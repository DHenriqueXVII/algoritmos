algoritmo "semnome"
var
  A,D:Inteiro
  B,C:Real
inicio
     A <- 0
     Escreva ("Quantas conversões fazer ?")
     Leia (D)
     Enquanto (A < D) faca
            A <- A + 1
            Escreva ("Quantos R$ você tem ?")
            Leia (B)
            C <- B/2.22
            Escreval ("Você pode comprar US$ ",C:4:2)
     FimEnquanto
fimalgoritmo
