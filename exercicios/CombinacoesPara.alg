algoritmo "semnome"
var
  C1,C2,Valor1,Valor2:Inteiro
inicio
     Escreva ("Informe um número:")
     Leia (Valor1)
     Escreva ("Informe outro número:")
     Leia (Valor2)
     Para C1 := Valor1 ate Valor2 faca
        Para C2 := Valor1 ate Valor2 faca
           Escreval (C1,C2)
           FimPara
     FimPara
fimalgoritmo
