algoritmo "semnome"
var
  A,B,C,D,F:Real
inicio
  A <- 0
  Enquanto (A < 10) faca
  A <- A + 1
  Escreva ("Informe o",A,"° valor:")
  Leia (B)
  Se (B > D) entao
    D <- B
  FimSe
  Se (B < B) entao
    F <- B
  FimSe
  C <- C + B
  FimEnquanto
  Escreval ("A soma entre todos os valores é",C)
  Escreval ("O maior valor é",D)
  Escreva ("O menor valor é",F)
fimalgoritmo
